<script type="application/javascript">
    function toggleNav() {
        const menu = document.getElementById('menu');
        if (menu.classList.contains('hidden')) {
            menu.classList.remove('hidden');
        } else {
            menu.classList.add('hidden');
        }
    }
</script>
<nav class="bg-black shadow-lg">
  <div class="max-w-[1174px] mx-auto bg-gray-900 sm:flex  sm:items-center sm:px-4 sm:py-3">
      <div class="flex items-center justify-between px-4 py-3 sm:p-0">
          <div>
              <div >
                  <a href="/" class="hover:bg-gray-800"><img class="h-8" src="/jaxnode.png" alt="JaxNode" /></a>
              </div>
          </div>
          <div class="sm:hidden">
              <button type="button" class="block text-gray-500 hover:text-white focus:text-white focus:outline-none" onclick="toggleNav()">
                  <svg class="h-6 w-6 fill-current" viewBox="0 0 24 24">
                      <path d="M4 5h16a1 1 0 0 1 0 2H4a1 1 0 1 1 0-2zm0 6h16a1 1 0 0 1 0 2H4a1 1 0 0 1 0-2zm0 6h16a1 1 0 0 1 0 2H4a1 1 0 0 1 0-2z"/>
                  </svg>
              </button>
          </div>
      </div>
      <div class="hidden px-2 pt-2 pb-4 ml-0 sm:ml-2 sm:flex sm:p-0" id="menu">
          <a href="/" class="block px-2 py-1 text-white font-semibold rounded hover:bg-gray-800">Home</a>
          <a href="/weather" class="mt-1 block px-2 py-1 text-white font-semibold rounded hover:bg-gray-800 sm:mt-0 sm:ml-2">Weather</a>
          <a href="/contact" class="mt-1 block px-2 py-1 text-white font-semibold rounded hover:bg-gray-800 sm:mt-0 sm:ml-2">Contact</a>
          <a href="/sponsors" class="mt-1 block px-2 py-1 text-white font-semibold rounded hover:bg-gray-800 sm:mt-0 sm:ml-2">Sponsors</a>
          <a href="/code" class="mt-1 block px-2 py-1 text-white font-semibold rounded hover:bg-gray-800 sm:mt-0 sm:ml-2">Code</a>
      </div>
  </div>
</nav>